    "codestyle-fix": "prettier --write src\n"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn ts-check && yarn lint-fix && yarn codestyle-fix"
    }
  },
  "commands": [
    "yarn build && yarn start"
  ],
